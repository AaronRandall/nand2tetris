/**
 * 8-way Or gate: out = in[0] Or in[1] Or ... Or in[7].
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a=in[0],b=in[1],out=w1);
    Or(a=in[2],b=in[3],out=w2);
    Or(a=in[4],b=in[5],out=w3);
    Or(a=in[6],b=in[7],out=w4);
    Or(a=w1,b=w2,out=out1);
    Or(a=w3,b=w4,out=out2);
    Or(a=out1,b=out2,out=out);
}
